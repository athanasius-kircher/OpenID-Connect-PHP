{
    "name": "jumbojett/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "require": {
        "php": ">=5.4",
        "phpseclib/phpseclib" : "2.0.1",
        "ext-json": "*",
        "ext-curl": "*",
        "psr/http-message": "^1.0"
    },
    "archive" : {
        "exclude" : [
            ".*"
        ]
    },
    "autoload" : {
         "classmap": [ "src/"]
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.6"
    }
}
